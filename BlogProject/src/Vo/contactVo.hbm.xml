<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	
	<class name="Vo.contactVo" table="contact">
	
		<id name="id" type="int" column="id">
			<generator class="increment"/>
		</id>

		<property name="name">
			<column name="name"/>
		</property>
		
		<property name="email">
			<column name="email"/>
		</property>
		
		<property name="phone">
			<column name="phone"/>
		</property>
		
		<property name="message">
			<column name="massage"/>
		</property>
		
		<property name="date">
			<column name="date"/>
		</property>
		
		
				
	</class>

</hibernate-mapping>